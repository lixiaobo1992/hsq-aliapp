<!--基础元素-->
<template name="aParseImg">
  <image class="{{item.classStr ? item.classStr : ''}} aParse-{{item.tag}}" data-from="{{item.from}}" data-src="{{item.attr.src}}" data-idx="{{item.imgIndex}}" src="{{item.attr.src}}" onLoad="aParseImgLoad" onTap="aParseImgTap" mode="scaleToFill" style="width:{{item.width}}px;height:{{item.height}}px;"
  />
</template>
