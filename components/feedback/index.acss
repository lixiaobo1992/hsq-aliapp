
.btn-feedback {
  position: fixed;
  z-index: 200;
  right: 0;
  bottom: 200rpx;
  line-height: 0;
  width: 54rpx;
  height: 190rpx;
  overflow: hidden;
  transform: translate3d(54rpx, 0, 0);
  transition: all 1s;
}
.btn-feedback-show{
  transform: translate3d(0, 0, 0);
}

.btn-feedback image {
  width: 100%;
  height: 100%;
}
