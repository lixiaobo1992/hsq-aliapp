.view-activty-bg {
  background-color: rgb(226,226,226);
}
.view-activty {
  position: relative;
  overflow: hidden;
  width: 100%;
}
.view-tip {
  display: flex;
  position: relative;
  width: 100%;
  height: 100rpx;
  background-color: rgb(250, 250, 248);
  justify-content: center;
  align-items: center;
}

.view-tip .tip-line {
  width: 100%;
  margin: 0 30rpx;
  height: 1px;
  background-color: #e2e2e2;
}

.view-tip .tip-title {
  position: absolute;
  left: 50%;
  top: 0;
  margin-left: -100rpx;
  width: 200rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  color: rgb(153, 153, 153);
  text-align: center;
  background-color: rgb(250, 250, 248);
}

.swiper{
  padding-top: 56.25%;
}

