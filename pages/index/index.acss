.activity-box{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}
.activity-block{
  width: 100%;
  padding-top: 112.6%;
  position: relative;
}
.activity-image{
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
}
.activity-image image{
  width: 100%;
  height: 100%;
}
.close-btn{
  position: absolute;
  top: -60rpx;
  right: 40rpx;
  border-radius: 100rpx;
  width: 60rpx;
  height: 60rpx;
}
.close-image{
  width: 100%;
  height: 100%;
}
.mask{
  position: fixed;
  left: 0px;
  top: 0px;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
}
.tip-surface .mask{
  background-color: rgba(0, 0, 0, 0.6);
}
.activity-tip, .coupon-tip{
  z-index: 10;
}
.tip-surface{
  z-index: 11;
}

.coupon-tip-list{
  width: 75%;
  height: 46.6%;
  position: absolute;
  top: 41%;
  left: 12.5%;
  overflow: hidden;
  overflow-y: scroll;
}
.coupon-item{
  width: 100%;
  padding-top: 26.1%;
  position: relative;
  margin-bottom: 10rpx;
}
.coupon-item image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.coupon-item-tit{
  word-break: break-all;
  white-space: normal;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 1;
  margin-right: 10rpx;
}
.coupon-item-tit .item-list{
  display:block;
  font-size:20rpx;
  white-space: nowrap;
  line-height:28rpx;
  align-items: center;
  text-align:left;
  padding-left:12rpx;
}
.coupon-left{
  width: 29.4%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  color: rgb(51, 51, 51);
}
.coupon-right{
  width: 70%;
  height: 100%;
  position: absolute;
  right: 0px;
  top: 0px;
}
.coupon-price{
  font-size: 24rpx;
  color: rgb(251, 75, 65);
}
.coupon-price text{
  font-size: 60rpx;
  font-weight:bold;
}
.coupon-right{
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 20rpx;
  color: rgb(129, 129, 129);
  padding-left: 10rpx;
}
.coupon-right-top{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color:#FB4B41;
  line-height: 40rpx;
  align-items: center;
  height: 60rpx;
}
.coupon-item-status{
  color: rgb(255, 50, 33);
  font-size: 20rpx;
  border-radius: 40rpx;
  border: 1rpx solid rgb(255, 50, 33);
  margin-right: 10rpx;
  line-height: 40rpx;
  width: 100rpx;
  height: 40rpx;
  text-align: center;
  flex-shrink: 0;
}

