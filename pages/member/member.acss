/* card */
.member-card{
  position: relative;
  width: 628rpx;
  height: 360rpx;
  background: url('https://img1.haoshiqi.net/miniapp/vip-card_303e9030e9.png') no-repeat top center;
  background-size: 100% 100%;
  margin: 0 auto;
  overflow: hidden;
  border-radius: 20rpx 20rpx 0 0;
}
/* 未开通vip */
.member-card.not-vip{
  background-image: url(' https://img1.haoshiqi.net/miniapp/vip-card1_32b5e6df30.png')
}
.member-card.not-vip .member-open-title{
  font-size: 68rpx;
  font-weight: bold;
  text-align: center;
  margin-top: 134rpx;
}

.member-card.not-vip .member-open-title .tit-text{
    background-image:-webkit-linear-gradient(bottom,#333,#AD7200);
  -webkit-background-clip:text;
  -webkit-text-fill-color:transparent;
  text-shadow:2rpx 2rpx 0.1em rgba(178,106,0,0.59);
}

.member-card.not-vip .member-open-title .tit-price{
  color: #E70000;
