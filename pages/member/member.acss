.dwd-popup{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9;
}

.dwd-popup .dwd-popup-mask{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 0;
  background-color: rgba(0, 0, 0, 0.7)
}
.dwd-popup .dwd-popup-content{
  top: 110%;
  left: 50%;
  position: fixed;
  max-height: 100%;
  overflow-y: auto;
  box-sizing: border-box;
  background-color: #fff;
  -webkit-overflow-scrolling: touch;
  z-index: 1;

  transition-property: all;
  transition-duration: 0.5s;
  transition-timing-function: ease-in-out;
}
.dwd-popup .dwd-popup-content.show{
  top: 50%;
}
.dwd-popup--center {
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
}


.dwd-popup .dwd-popup-content.open-popup-conent{
  width: 80%;
  height: 560rpx;
  overflow: hidden;
  background: url('https://img1.haoshiqi.net/miniapp/open-popups_44261d566a.png') no-repeat left top;
  background-size: 100% auto;
  padding-top: 250rpx;
  text-align: center;
}

.open-popup-conent .open-tit{
  font-size: 40rpx;
  color: #333333;
  margin-bottom: 20rpx;
}
.open-popup-conent .open-sub{
  font-size: 28rpx;
  color: #666666;
}
.open-popup-conent .open-member-btn{
  width: 85%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #333333;
  box-shadow: 0 4rpx 12rpx 0 rgba(0,0,0,0.31);
  border-radius: 28px;
  margin: 40rpx auto 20rpx auto;
  color: #fff;
  font-size: 36rpx;
}

.dwd-popup .dwd-popup-content.rule-popup-conent{
  width: 80%;
  height: 60%;
  overflow: visible;
  background-color: #fff;
  border-radius: 20rpx;
  padding: 30rpx;
  position: relative;
}

.rule-conent{
  height: 100%;
  width: 100%;
  overflow-y: scroll;
}
.rule-popup-conent .rule-block{
  margin-bottom: 20rpx;
}
.rule-popup-conent .rule-block .rule-tit{
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.rule-popup-conent .rule-block .rule-subtit{
  font-size: 28rpx;
