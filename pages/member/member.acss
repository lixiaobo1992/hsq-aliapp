  z-index: 1;
}

/* 一个叠加的样式 */
.header-article{
  width: 100%;
  position: relative;
  height: 44rpx;
}
.header-article::after{
  content: '';
  display: block;
  position: absolute;
  left: 5%;
  bottom: -42rpx;
  width: 90%;
  height: 60rpx;
  opacity: 0.83;
  background-color: #F9EED9;
  border-radius: 30rpx;
  z-index: 1;
}
.header-article::before{
  content: '';
  display: block;
  position: absolute;
  left: 10%;
  bottom: -18rpx;
  width: 80%;
  height: 60rpx;
  opacity: 0.54;
  background-color: #F9EED9;
  border-radius: 30rpx;
  z-index: 0;
}

.member-coupon-content{
  border: 1px solid #fff;
  box-shadow: 0 18rpx 16rpx 0 #F6EBD9;
  border-radius: 30rpx;
  position: relative;
  z-index: 2;
  background: url('https://img1.haoshiqi.net/miniapp/texture2_718c1849fb.png') no-repeat top right #fff;
  background-size: 40% auto;
  padding-bottom: 40rpx;
}

.member-coupon-content .rights-coupon-item{}

