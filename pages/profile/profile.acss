.more{
  float: right;
  display: block;
  width: 25rpx;
  height: 25rpx;
  margin-top: 26rpx;
  margin-left: 16rpx;
  background-image: url('data:image/svg+xml;charset=utf-8,<svg width="16" height="26" viewBox="0 0 16 26" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g id="UI-KIT_基础元件" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g id="9.9基础元件" transform="translate(-5809.000000, -8482.000000)" fill="#999999"><polygon id="Disclosure-Indicator" points="5811 8482 5809 8484 5820.5 8495 5809 8506 5811 8508 5825 8495"></polygon></g></g></svg>');
  background-size: contain;
  background-repeat: no-repeat;
  background-position: 50% 50%;
  visibility: visible;
  transition: transform 0.3s;
}

.pr{
  position: relative;
}

.save-money{
  width: 100%;
  padding: 0 30rpx;
  line-height: 80rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
}
.save-money image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
.save-text{
  color: #666;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
}
.save-text text{
  font-family: PingFangSC-Medium;
  color: #EFB40A;
}
