.app-page{
  position: relative;
  width: 100%;
  padding: 0;
  margin: 0;
  font-size:30rpx;
}
.profile-top{
  width: 100%;
  height: 356rpx;
  background-image: linear-gradient(90deg,#fff 0%, #FFEBEB 100%);
  /* background-image: url("https://img1.haoshiqi.net/miniapp/img/proflie_vip_85ed879c46.png"); */
  /* background-size:100% 142rpx;
  background-repeat:no-repeat;
  background-position:20rpx 40rpx; */
}
.profile-top image{
  width: 100%;
  height: 242rpx;
}


.all-order{
  overflow: hidden;
}
.order-box{
  display:flex;
  border-top: 1px solid #e2e2e2;
}
.order-box .pay-item{
  flex: 1;
  text-align: center;
  padding: 18rpx 0rpx 0rpx;
  background: #fff;
}
.pay-item .pay-icon{
  width: 48rpx;
  height: 48rpx;
  display:inline-block;
  margin-bottom:8rpx;
  position: relative;
}
.pay-item .order-num{
  position: absolute;
  left: 24rpx;
  top: 4rpx;
  color: #fff;
  background-image: linear-gradient(-90deg, #FF0000 4%, #FF5F5F 100%);
  border-radius: 16rpx;
  height: 32rpx;
  line-height: 32rpx;
  padding:0 9rpx;
  font-size: 24rpx;
  white-space: nowrap;
}
.pay-icon .icon-img{
  width: 100%;
  height: 100%;
}
.pay-item .pay-state{
  display:block;
  color: #666;
  margin-top: -20rpx;
  white-space: nowrap;
  font-size:24rpx;
}
.sell-box{
  background-color: #fff;
  font-size: 28rpx;
  line-height: 80rpx;
  margin:32.6rpx 24rpx;
  box-shadow: 0 4rpx 10rpx 0 rgba(0,0,0,0.10);
  border-radius:12rpx;
}
.sell-box .order-left{
  font-weight:bold;
}
.sell-box .order-right{
  color:#999;
  font-size:24rpx;
}
.box-pad{
  padding: 0 24rpx;
}
.ali-company-info{
  font-size: 24rpx;
  margin-top:40rpx;
  text-align: center;
  color: #959595;
}

.user-id{
  font-size: 28rpx;
  text-align: center;
  color: #959595;
  margin-top:30rpx;
  margin-bottom: 30rpx;
}

