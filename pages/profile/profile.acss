.profile-top .top-info{
  display:flex;
  flex-direction: row;
  justify-content: flex-start;
  padding:32rpx 40rpx;
  margin-top:-160rpx;
  position: relative;
}
.profile-top .user-info{
  padding: 20rpx 0rpx 0 40rpx;
  flex: 1;
}

.setting-btn{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 30rpx;
  top: 20rpx;
}
.setting-btn image{
  width: 100%;
  height: 100%;
}


.profile-top .user-name{
  display:flex;
  flex-direction: row;
  font-weight:bold;
  font-size:36rpx;
}
.user-info .user-name>text{
  max-width:300rpx;
  height:50rpx;
  line-height:50rpx;
  display:inline-block;
  overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.profile-top .user-id{
  font-size:24rpx;
  color:#666;
}
.profile-top .user-avator{
  width:120rpx;
  height:120rpx;
  border-radius:50%;
}
.profile-top .vip-status{
  display:inline-block;
  /* background-image: linear-gradient(-270deg, #FE0505 0%, #FA5A5A 100%); */
  width:128rpx;
  height:36rpx;
  border-radius:18rpx;
  margin-left:12rpx;
  vertical-align: bottom;
}
.profile-top .vip-tag{
  width:128rpx;
  height:36rpx;
  margin-top:-2rpx;
}
.profile-top .botton-item{
  display:flex;
  flex-direction: row;
  justify-content: space-between;
  width:100%;
  margin: 0 auto;
}
.profile-top .list{
  padding:0rpx 30rpx 0 40rpx;
  border-right:1rpx solid #d7d7d7;
  width:33.3%;
  text-align: center;
}
.profile-top .last{
  border-right:none;
}
.profile-top .list-num{
  text-align:center;
  display:block;
  font-weight:bold;
}
.profile-top .list-name{
  font-size:22rpx;
  color:#999;
}
.guess-title{
  font-size: 32rpx;
  color: #222222;
  display:block;
  text-align:center;
  padding:0 0 40rpx;
  font-weight:bold;
}
