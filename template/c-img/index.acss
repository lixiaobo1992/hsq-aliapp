
.c-img{
  display: block;
  line-height: 0;
  font-size: 0;
  padding: 0;
  margin: 0;
  outline: none;
  overflow: hidden;
}

.c-img-item{
  display: block;
  position: relative;
  width: 100%;
  line-height: 0;
  font-size: 0;
  padding: 0;
  margin: 0;
  outline: none;
  overflow: hidden;
}
.c-img-item image{
  display: block;
  width: 100%;
  line-height: 0;
  font-size: 0;
  padding: 0;
  margin: 0;
  outline: none;
  overflow: hidden;
  vertical-align: top;
}
